{
	"[G] Aligned": {
		"prefix": "ali",
		"body": [
			"\\begin{aligned}",
			"\t${1:$TM_SELECTED_TEXT}",
			"\\end{aligned}"
		]
	},
	"[G] Gathered": {
		"prefix": "gath",
